                                        <RadioButton Content="自分の演奏のみ" Height="16" HorizontalAlignment="Left" Margin="358,6,0,0" VerticalAlignment="Top" />
                                    </Grid>
                                    <CheckBox Content="足跡サウンド使用" Height="16" HorizontalAlignment="Left" Margin="6,204,0,0" VerticalAlignment="Top" />
                                </Grid>
                            </TabItem>
                            <TabItem Header="性能">
