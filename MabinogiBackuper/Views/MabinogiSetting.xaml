                                        <Slider Height="23" HorizontalAlignment="Right" Margin="0,6,46,0" VerticalAlignment="Top" Width="294" SmallChange="1" Minimum="10" Maximum="60" />
                                        <CheckBox Content="パーティーメンバーを簡素化しない" Height="16" HorizontalAlignment="Left" Margin="6,42,0,0" VerticalAlignment="Top" />
                                    </Grid>
                                    <Label Content="半神化エフェクト" Height="28" HorizontalAlignment="Left" Margin="6,209,0,0" VerticalAlignment="Top" />
                                    <ComboBox Height="24" HorizontalAlignment="Left" Margin="25,235,0,0" VerticalAlignment="Top" Width="505" />
