                                        <Slider Height="23" Margin="165,6,46,0" VerticalAlignment="Top" IsDirectionReversed="True" Maximum="300" Minimum="20" SmallChange="1" />
                                        <Label Content="速く" Height="28" HorizontalAlignment="Right" Margin="0,6,6,0" VerticalAlignment="Top" />
                                    </Grid>
                                    <CheckBox Content="エフェクト簡素化" Height="16" HorizontalAlignment="Left" Margin="6,341,0,0" VerticalAlignment="Top" />
